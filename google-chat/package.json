{
  "name": "google-chat-ces-bot",
  "version": "1.0.0",
  "description": "Google Chat Integration for CX Agent Studio",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "deploy:functions": "gcloud functions deploy handleChat --runtime nodejs18 --trigger-http --entry-point handleChat",
    "deploy:run": "gcloud run deploy google-chat-ces-bot --source ."
  },
  "keywords": [
    "google-chat",
    "chatbot",
    "cx-agent-studio",
    "google-cloud",
    "conversational-ai",
    "workspace"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/ces": "^0.1.0",
    "@googleapis/chat": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.4.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
